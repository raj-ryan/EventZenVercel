{
  "version": 2,
  "framework": null,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/public" } }
  ],
  "routes": [
    { "src": "/api/events", "dest": "/api/events.js" },
    { "src": "/api/events/(.*)", "dest": "/api/events.js" },
    { "src": "/api/venues", "dest": "/api/venues.js" },
    { "src": "/api/venues/(.*)", "dest": "/api/venues.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 