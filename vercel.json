{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    { "src": "/api/ping", "dest": "/api/ping.js" },
    { "src": "/api/events", "dest": "/api/events.js" },
    { "src": "/api/events/(.*)", "dest": "/api/events.js" },
    { "src": "/api/venues", "dest": "/api/venues.js" },
    { "src": "/api/venues/(.*)", "dest": "/api/venues.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json))", "dest": "/dist/public/$1" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 